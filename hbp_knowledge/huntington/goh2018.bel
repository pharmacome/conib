##############
# Statements #
##############
SET Citation = {"PubMed", "30012004"}

SET Evidence = "Gradual atrophy of the striatum (caudate and putamen)
due to neuronal loss is the neuropathological and neuro-
radiological  hallmark  of  HD."
SET Confidence = "High"
SET MeSHAnatomy = "Corpus Striatum"
bp(GO:"neuron death") -> path(MESH:Atrophy)
bp(GO:"neuron death") pos path(MESH:"Huntington Disease")
path(MESH:Atrophy) pos path(MESH:"Huntington Disease")
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "Specific  loss  of  medium
spiny neurons in the dorsal striatum, expressing either
Dopaminergic type 1 or 2 receptors, is seen as the basis
for the clinical phenotype with disease progression (see
Figure 1). "
SET Confidence = "High"
SET Cell = "medium spiny neuron"
SET MeSHAnatomy = "Neostriatum"
bp(GO:"neuron death") pos path(MESH:"Huntington Disease")
UNSET MeSHAnatomy
UNSET Cell
UNSET Confidence

SET Evidence = "The exact mechanism of HD pathogen-
esis is currently unknown, but putative theories include
excitotoxicity, oxidative stress, impaired energy metabo-
lism, mitochondrial dysfunction, inflammatory events,
apoptosis,  autophagy,  abnormal  protein–protein  inter-
actions,  disruption  of  dopamine,  glutamate,  cannabi-
noid and adenosine systems. 11"
SET Confidence = "Medium"
path(HBP:"Excitotoxicity") pos path(MESH:"Huntington Disease")
path(MESH:"Oxidative Stress") pos path(MESH:"Huntington Disease")
bp(MESH:"Energy Metabolism") neg path(MESH:"Huntington Disease")
path(HBP:"mitochondrial dysfunction") pos path(MESH:"Huntington Disease")
path(MESH:"Inflammation") pos path(MESH:"Huntington Disease")
bp(GO:"apoptotic process") pos path(MESH:"Huntington Disease")
bp(GO:"autophagy") pos path(MESH:"Huntington Disease")
bp(GO:"dopamine metabolic process") neg path(MESH:"Huntington Disease")
bp(GO:"D-glutamate metabolic process") neg path(MESH:"Huntington Disease")
bp(GO:"cannabinoid biosynthetic process") neg path(MESH:"Huntington Disease")
bp(GO:"adenosine catabolic process") neg path(MESH:"Huntington Disease")
UNSET Confidence

SET Evidence = "Significant cere-
bral cortical grey and white matter loss in patients with
HD is also common, particularly in frontal zones. 13"
SET Confidence = "High"
SET MeSHAnatomy = "Cerebral Cortex"
path(MESH:"Huntington Disease") neg a(MESH:"Gray Matter")
path(MESH:"Huntington Disease") neg a(MESH:"White Matter")
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "These
motor  symptoms  progressively  worsen  during  the
course of the disease, and cause significant difficulties
for  activities  of  daily  life  and  function,  resulting  in
increasing disability (see Table 3)."
SET Confidence = "High"
path(MESH:"Huntington Disease") neg bp(GO:"motor behavior")
UNSET Confidence

SET Evidence = "The cognitive symptoms in HD primarily reflect a form of
subcortical  dementia  characterized  by  memory  deficits,
psychomotor slowing and impairment in executive, per-
ceptual and spatial skills."
SET Confidence = "High"
path(MESH:"Huntington Disease") pos path(MESH:"Dementia")
path(MESH:"Huntington Disease") neg bp(GO:memory)
path(MESH:"Huntington Disease") neg bp(GO:"psychomotor behavior")
path(MESH:"Huntington Disease") neg bp(GO:"sensory perception")
path(MESH:"Huntington Disease") neg bp(GO:"visual learning")
UNSET Confidence
