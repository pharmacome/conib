# Statements #
##############

SET Citation = {"PubMed","29988016"}

SET Evidence = " Thus, for our initial studies we engineered
and purified full-length (FL) tau monomer that lacks any internal cysteines due to alanine substitu-
tions (C299A and C322A), termed tau (2A). FL tau (2A) cannot self-associate based on disulfide link-
ages, which helped prevent the formation of cryptic dimers that could have confounded our studies."
SET Confidence = "Medium"
p(HGNC:MAPT, sub(C,299,A), sub(C,322,A)) -| complex(p(HGNC:MAPT, sub(C,299,A), sub(C,322,A)),p(HGNC:MAPT, sub(C,299,A), sub(C,322,A)))
UNSET Confidence

SET Evidence = " Upon incubation
with Lipofectamine, we were surprised to observe seeding by monomer and larger assemblies alike,
whether FL WT or 2A. (Figure 1C,D)."
SET Confidence = "High"
a(PUBCHEM:"100984821") -> a(HBP:"Tau aggregates")
UNSET Confidence

SET Evidence = "However Ms induced amyloid forma-
tion, albeit more slowly than trimer or unfractionated fibrils (Figure 1F)."
#Ms is seeding active monomers
SET Confidence = "Medium"
act(p(HGNC:MAPT, var("?"))) -> a(HBP:"Tau aggregates")
a(HBP:"Tau fibrils") -> a(HBP:"Tau aggregates")
complex(p(HGNC:MAPT),p(HGNC:MAPT),p(HGNC:MAPT)) -> a(HBP:"Tau aggregates")
UNSET Confidence

SET Evidence = "After 15 min of heparin exposure, we detected low but significant amounts of seed-compe-
tent monomer, and much fewer larger assemblies (Figure 6A)."
SET Confidence = "Medium"
a(CHEBI:heparin) -> act(p(HGNC:MAPT, var("?")))
a(CHEBI:heparin) neg a(HBP:"Tau aggregates")
UNSET Confidence

#SET Evidence = "In M s , we
#observed a consistent crosslinking of K150 with K254, K267, K274 or K280 all located between RD 1 and 2,"
#crosslinking? is p(HGNC:MAPT,var("?"))

SET Evidence = " Tau from control brain purified in the monomer fraction (Figure 8A), while tau
from AD brain distributed across multiple fractions, corresponding to monomer and larger assem-
blies (Figure 8B). "
SET Confidence = "High"
SET MeSHAnatomy = "Brain"
path(MESH:"Alzheimer Disease") pos a(HBP:"Tau aggregates")
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "AD-derived M s that was purified, frozen, and re-purified by SEC exhibited seeding activ-
ity exclusively in the monomer fraction (Figure 8E). By contrast, AD-derived M s incubated at RT
formed seed-competent assemblies of increasing size (Figure 8E)."
SET Confidence = "Medium"
SET MeSHDisease = "Alzheimer Disease"
act(p(HGNC:MAPT, var("?"))) -> a(HBP:"Tau aggregates")
UNSET MeSHDisease
UNSET Confidence

