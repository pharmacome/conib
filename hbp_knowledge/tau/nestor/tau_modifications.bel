p(HGNC:MAPT, pmod(Me, Lys, 353)) reg complex(p(HGNC:MAPT),a(GO:microtubule))
p(FPLX:AMPK) reg complex(p(HGNC:MAPT),a(GO:microtubule))
UNSET Tau_Motif
SET Tau_Structure = "projection domain"
