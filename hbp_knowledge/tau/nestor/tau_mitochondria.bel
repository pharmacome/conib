r(MGI:Dnm1l) neg r(MGI:Synpo2)
r(MGI:Dnm1l) neg r(MGI:Nrgn)
r(MGI:Dnm1l) neg r(MGI:Gap43)

r(MGI:Syp) pos act(complex(GO:"neuron to neuron synapse"))
