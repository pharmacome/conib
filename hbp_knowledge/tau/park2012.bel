a(CHEBI:"AICA ribonucleotide") -> act(p(FPLX:AMPK))
a(CHEBI:"AICA ribonucleotide") -> p(HGNC:GSK3B,pmod(Ph,Ser,9))
act(p(FPLX:AMPK)) -> p(HGNC:GSK3B,pmod(Ph,Ser,9))
a(CHEBI:"AICA ribonucleotide") -| p(HGNC:MAPT,pmod(Ph,Ser,396))
act(p(FPLX:AMPK)) -| p(HGNC:MAPT,pmod(Ph,Ser,396))
a(CHEBI:"AICA ribonucleotide") -| p(HGNC:MAPT,pmod(Ph,Ser,404))
act(p(FPLX:AMPK)) -| p(HGNC:MAPT,pmod(Ph,Ser,404))
UNSET MeSHAnatomy
