bp(GO:autophagy) -> deg(p(HGNC:HTT, var("?")))
p(HGNC:HTT, var("?")) -| bp(GO:autophagy)
p(HGNC:HTT, var("?")) -| bp(GO:"cargo loading into vesicle")
p(HGNC:HTT, var("?")) -| bp(GO:"autophagosome assembly")
UNSET Confidence

