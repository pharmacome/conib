complex(p(HGNC:DNM1L),p(HGNC:SOD1,var("?"))) neg path(MESH:"Mitochondrial Dynamics")
UNSET MeSHDisease
UNSET Confidence

SET Species = "10090"

SET Evidence = "Phosphorylation at Ser‐40 and Ser‐44 mediated
by glycogen synthase kinase 3β (GSK3β) can promote GTPase activity
of Drp1 leading to an increase in mitochondrial division causing
neuronal cell apoptosis induced by increased vulnerability to Aβ toxicity
in the AD mice model"
SET Confidence = "High"
SET MeSHDisease = "Alzheimer Disease"
p(HGNC:GSK3B) -> p(HGNC:DNM1L,pmod(Ph,Ser,40),pmod(Ph,Ser,44))
p(HGNC:DNM1L,pmod(Ph,Ser,40),pmod(Ph,Ser,44)) -> act(p(HGNC:DNM1L),ma(GO:"GTPase activity"))
p(HGNC:GSK3B) -> act(p(HGNC:DNM1L),ma(GO:"GTPase activity"))
act(p(HGNC:DNM1L),ma(GO:"GTPase activity")) -> bp(GO:"mitochondrial fission")
