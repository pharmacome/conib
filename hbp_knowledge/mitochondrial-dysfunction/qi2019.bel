act(p(HGNC:MIEF1)) -| a(CHEBI:"reactive oxygen species")
act(p(HGNC:MFF)) -| a(CHEBI:"reactive oxygen species")
act(p(HGNC:FIS1)) -| a(CHEBI:"reactive oxygen species")
act(p(HGNC:DNM1L)) -| a(CHEBI:"reactive oxygen species")
act(p(HGNC:MIEF2)) -| path(DOID:"neurodegenerative disease")
act(p(HGNC:MIEF1)) -| path(DOID:"neurodegenerative disease")
act(p(HGNC:MFF)) -| path(DOID:"neurodegenerative disease")
act(p(HGNC:FIS1)) -| path(DOID:"neurodegenerative disease")
act(p(HGNC:DNM1L)) -| path(DOID:"neurodegenerative disease")
a(CHEBI:"reactive oxygen species") -> path(DOID:"neurodegenerative disease")
bp(GO:"energy homeostasis") -| path(DOID:"neurodegenerative disease")
UNSET Confidence

SET Evidence = "Studies suggest that Drp1 can interact
with some key pathogenic proteins‐Aβ, mutant PINK/Parkin, mutant
Huntingtin (Htt), and mutant superoxide dismutase 1 (SOD1) important
in the pathogenesis of AD, PD, HD, and ALS, causing the dysfunction of
mitochondrial dynamics in these diseases."
SET Confidence = "High"
SET MeSHDisease = "Alzheimer Disease"
complex(p(HGNC:DNM1L),a(CHEBI:"amyloid-beta")) neg path(MESH:"Mitochondrial Dynamics")
UNSET MeSHDisease
