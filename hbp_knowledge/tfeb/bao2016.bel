p(HGNC:TFEB,var("p.Lys116Arg")) cnc deg(p(CONSO:"amyloid-beta fibrils"))
p(HGNC:TFEB,var("p.Lys116Arg")) cnc a(MESH:Lysosomes)
p(HGNC:TFEB,var("p.Lys116Arg")) cnc act(a(MESH:Lysosomes))

