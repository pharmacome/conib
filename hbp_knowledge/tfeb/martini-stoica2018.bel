SET Confidence = "High"
p(HGNC:TFEB) -> a(MESH:Lysosomes)
p(HGNC:TFEB) pos bp(GO:"lysosomal lumen acidification")
UNSET Confidence
UNSET Species

SET MeSHAnatomy = "Astrocytes"

SET Evidence = "Confocal microscopy revealed an expected increase in
