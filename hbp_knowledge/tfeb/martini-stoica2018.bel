SET Confidence = "High"
composite(p(HGNC:TFEB),a(MESH:Astrocytes)) -> composite(a(PUBCHEM:102513886,loc(MESH:Lysosomes)),p(MESH:"Serum Albumin, Bovine",loc(MESH:Lysosomes)))
SET MeSHAnatomy = "Lysosomes"
