p(HGNC:TFEB) -> p(CONSO:"Tau fibrils",loc(MESH:Lysosomes))
p(CONSO:"Tau fibrils",loc(MESH:Lysosomes)) -> deg(p(CONSO:"Tau fibrils"))
UNSET Confidence

SET Evidence = "Flow cytometry analysis of
