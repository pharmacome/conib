##############
# Statements #
##############
SET Citation = {"PubMed", "4255369"}

#performed in Balb/c mice  and transgenic R3m/4 strain expressing human truncated tau
SET Species = "10090"

SET Evidence = "The antibody
reduced the amount of oligomerised truncated tau
by 84% when measured by thioflavin T fluorescence
(Table 2)."
#antibody - DC8E8
p(CONSO:DC8E8) -| p(HGNC:MAPT,pmod(CONSO:"Tau oligomers"),frag("?"))

SET Evidence = "Surprisingly, within the panel of screened antibodies
targeting mis-disordered tau, some antibodies
displayed an opposite effect, that is, not inhibition but enhancement
of tau oligomerisation (Table 2, mAb MN423
and DC11)."
a(CONSO:MN423) -> a(CONSO:"Tau oligomers")
a(CONSO:DC11) -> a(CONSO:"Tau oligomers")

SET Evidence = "The analysis
showed that DC8E8 inhibited the whole process of tau
oligomerisation, that is, the formation of tau dimers, trimers
and higher-order oligomers of mis-disordered tau
(Figure 1B)."
p(CONSO:DC8E8) -| a(CONSO:"Tau oligomers")
p(CONSO:DC8E8) -| complex(a(HGNC:MAPT),a(HGNC:MAPT))
p(CONSO:DC8E8) -| complex(a(HGNC:MAPT),a(HGNC:MAPT),a(HGNC:MAPT))
p(CONSO:DC8E8) -| p(HGNC:MAPT,pmod(CONSO:misfolded),pmod(CONSO:"Tau oligomers"))
a(CONSO:"Tau oligomers") -> a(CONSO:"Tau fibrils")
p(CONSO:DC8E8) -| a(CONSO:"Tau fibrils")

SET Evidence  = "Immunohistochemical analysis showed that DC8E8 enabled
us to detect early stages of pathological tau in human
preclinical AD (Braak stage I) (Figure 2A)."
p(CONSO:DC8E8) pos path(DOID:"Alzheimer's disease")

SET Evidence = "The transgenic mice show an age-dependent
development of NFTs and formation of sarcosyl-insoluble
tau oligomers in the brainstem that are associated
with the onset of progressive motor dysfunction
leading to premature death at 6 to 7 months of age."
#line R3m/4 expressing mis-disordered human tau under Thy1 promoter
bp(GO:aging) pos a(CONSO:"Tau fibrils")
SET Anatomy = "brainstem"
#sarcosyl-insoluble tau oligomers
bp(GO:aging) pos a(CONSO:"Tau oligomers")
a(CONSO:"Tau oligomers") -> path(MESH:"Motor Disorders")
#age 6-7 months
path(MESH:"Motor Disorders") -> bp(GO:"cell death")
UNSET Anatomy

SET Evidence = "We found
that DC8E8 therapy significantly reduced the levels of
sarcosyl-insoluble tau oligomers (89%) (Figure 3C and D).
Strikingly, total tau levels did not differ between the groups
(Figure 3A and B)."
p(CONSO:DC8E8) =| a(CONSO:"Tau oligomers")
p(CONSO:DC8E8) cnc p(HGNC:MAPT)

SET Evidence = "Furthermore, DC8E8
displays a significant preference for mis-disordered tau
protein deletion mutants (151-391/4R, 297-391/4R, 1-357/
4R and 221-441/4R) over the physiological full-length tau
isoforms 2N4R and 2N3R (Figure 5A)."
#with resect to binding
p(CONSO:DC8E8) => complex(p(CONSO:DC8E8),p(CONSO:"4R tau",var("p.151_391del")))
p(CONSO:DC8E8) => complex(p(CONSO:DC8E8),p(CONSO:"4R tau",var("p.297_391del")))
p(CONSO:DC8E8) => complex(p(CONSO:DC8E8),p(CONSO:"4R tau",var("p.1_357del")))
p(CONSO:DC8E8) => complex(p(CONSO:DC8E8),p(CONSO:"4R tau",var("p.221_441del")))
p(CONSO:DC8E8) -> complex(p(CONSO:DC8E8),p(CONSO:"Tau isoform F (441 aa)"))
p(CONSO:DC8E8) -> complex(p(CONSO:DC8E8),p(CONSO:"Tau isoform C (410 aa)"))

SET Evidence = "DC8E8 did not recognise
tau constructs where the MTBR region was deleted
(tau 2N4RΔ(222-427) and tau 2N4RΔ(257-400))."
p(CONSO:DC8E8) -| complex(p(CONSO:DC8E8),p(CONSO:"Tau isoform F (441 aa)",var("p.222_427del")))
p(CONSO:DC8E8) -| complex(p(CONSO:DC8E8),p(CONSO:"Tau isoform F (441 aa)",var("p.257_400del")))
