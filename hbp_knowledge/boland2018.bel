ments interfere with CMA 45,47 ."
bp(GO:"chaperone-mediated autophagy") -> deg(p(HGNC:MAPT))
a(HBP:HBP00006) -| bp(GO:"chaperone-mediated autophagy")
p(HGNC:MAPT, var("?")) -| bp(GO:"chaperone-mediated autophagy")
p(HGNC:MAPT, frag("?")) -| bp(GO:"chaperone-mediated autophagy")

SET MeSHDisease = "Parkinson Disease"

SET Evidence = "First, autosomal 
