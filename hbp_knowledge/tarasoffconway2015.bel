p(HGNC:ACE) -| a(CHEBI:"amyloid-beta",loc(GO:intracellular))
p(HGNC:IDE) -| a(CHEBI:"amyloid-beta",loc(GO:intracellular))

SET Evidence =" Specifically, ISF Aβ can be taken up by microglia and astrocytes, whereas perivascular Aβ can be degraded by vascular smooth muscle cells, perivascular macrophages, and astrocytes"
a(MESH:Microglia) -| a(CHEBI:"amyloid-beta",loc(MESH:"Extracellular Fluid"))
a(MESH:Astrocytes) -| a(CHEBI:"amyloid-beta",loc(MESH:"Extracellular Fluid"))
a(MESH:"Muscle, Smooth, Vascular") -| a(CHEBI:"amyloid-beta")

