##############
# Statements #
##############
SET Citation = {"PubMed", "27758088"}

SET Evidence = " While CHRNA5 and CHRNA3 are
expressed in multiple brain regions, CHRNB4 mRNA is detectable only in peripheral
tissues; therefore, our subsequent analyses of CHRNB4 are restricted to peripheral tissues."
r(HGNC:CHRNA5) -- a(MESH:Brain)
r(HGNC:CHRNA3) -- a(MESH:Brain)
r(HGNC:CHRNB4) -- a(UBERON:0000010)

SET Evidence = "In both tissues CHRNA3, CHRNA5, and RP11-650L12.2 are significantly co-expressed
(Supp. Figure S2)."
#both tissues:skeletal muscle (representing the periphery) and nucleus accumbens
SET MeSHAnatomy = {"Muscle, Skeletal","Nucleus Accumbens"}
r(HGNC:CHRNA5) -- r(HGNC:CHRNA3)
r(MESH:"long non-coding RNA RP11-650L12.2, human") -- r(HGNC:CHRNA3)
r(MESH:"long non-coding RNA RP11-650L12.2, human") -- r(HGNC:CHRNA5)
UNSET MeSHAnatomy

SET Evidence = " therefore,
we select for further analysis the haplotype-tagging SNP rs880395, previously shown to be
associated with a near fourfold increase in CHRNA5 mRNA expression in brain (Smith et
al. 2011)."
SET MeSHAnatomy = "Brain"
g(DBSNP:rs880395) -> r(HGNC:CHRNA5)
UNSET MeSHAnatomy

SET Evidence = "The same CHRNA5
-enhancer SNP was also the strongest signal for
RP11-650L12.2 expression across several tissues, and for CHRNA3 in skeletal muscle but
not in nucleus accumbens (Supp. Table S4)."
#same SNP = rs880395
g(DBSNP:rs880395) -> r(MESH:"long non-coding RNA RP11-650L12.2, human")
SET MeSHAnatomy = "Nucleus Accumbens"
g(DBSNP:rs880395) -> r(HGNC:CHRNA3)
SET MeSHAnatomy = "Muscle, Skeletal"
g(DBSNP:rs880395) cnc r(HGNC:CHRNA3)
UNSET MeSHAnatomy

SET Evidence = " The CHRNA5-enhancer variant increases the nicotinic genes, but
decreases PSMA4 expression in heart, artery, skin and skeletal muscle (Supp. Table S4)."
#CHRNA5-enhancer variant = rs880395
SET MeSHAnatomy = {"Heart","Arteries","Skin","Muscle, Skeletal"}
g(DBSNP:rs880395) -| p(HGNC:PSMA4)
UNSET MeSHAnatomy

SET Evidence = "The CHRNA5-enhancer variant
(marked by rs880395) predominantly affects CHRNA3, CHRNA5, and RP11-650L12.2
RNA expression in muscle and other tissues, while in the nucleus accumbens, the
CHRNA3-enhancer haplotypes (rs1948) affects CHRNA3 (Figure 3A) and RP11-650L12.2 RNA."
g(DBSNP:rs880395) -> r(HGNC:CHRNA3)
g(DBSNP:rs880395) -> r(MESH:"long non-coding RNA RP11-650L12.2, human")
g(DBSNP:rs880395) -> r(HGNC:CHRNA5)
SET MeSHAnatomy = "Nucleus Accumbens"
g(DBSNP:rs1948) -> r(HGNC:CHRNA3)
g(DBSNP:rs1948) -> r(MESH:"long non-coding RNA RP11-650L12.2, human")
UNSET MeSHAnatomy

SET Evidence = "rs1948 is associated with higher CHRNA3 expression in the
nucleus accumbens (p=2.5e-17, effect size =0.99), which is further supported by considering
allelic mRNA ratios (Supp. Figure S6)."
SET MeSHAnatomy = "Nucleus Accumbens"
g(DBSNP:rs1948) -> r(HGNC:CHRNA3)
UNSET MeSHAnatomy

#rs880395 (labeled CHRNA5-enhancer)
#rs1948 (CHRNA3-enhancer)

SET Evidence = "Testing each SNP using ANOVA confirmed that rs16969968 is a risk factor for nicotine
dependence (Bierut et al. 2008; Smith et al. 2011)."
g(DBSNP:rs16969968) -> path(MESH:"Tobacco Use Disorder")

SET Evidence = "Considering only the most significant models from the ANOVA, the minor allele (A)
of rs16969968 is associated with risk of nicotine dependence, the minor allele (A) of
rs880395 is protective (negative effect size), and the minor allele (A) of rs1948 is associated
with risk (Table 2)."
g(DBSNP:rs16969968) -> path(MESH:"Tobacco Use Disorder")
g(DBSNP:rs880395) -| path(MESH:"Tobacco Use Disorder")
g(DBSNP:rs1948) -> path(MESH:"Tobacco Use Disorder")

SET Evidence = "The results support a protective role for the rs880395 A allele with higher expression of
CHRNA5 and suggest that the rs1948 A allele, similarly causing higher expression of
CHRNA3 mRNA, contributes to risk (Figure 5)."
#protection for path(MESH:"Tobacco Use Disorder")
g(DBSNP:rs880395) -> r(HGNC:CHRNA5)
composite(g(DBSNP:rs880395), r(HGNC:CHRNA5)) -| path(MESH:"Tobacco Use Disorder")
g(DBSNP:rs1948) -> r(HGNC:CHRNA3)
composite(g(DBSNP:rs1948), r(HGNC:CHRNA3)) -> path(MESH:"Tobacco Use Disorder")
