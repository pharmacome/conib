SET Citation = {"PubMed", "25652642"}

SET Evidence = "The constitutive form is transcriptionally active as evidenced by the nuclear
localization of the p50 and p65 subunits in the neurons of the cortex and hippocampus"
SET Confidence = "Medium"
SET MeSHAnatomy = {"Cerebral Cortex","Hippocampus"}
complex(p(HGNC:NFKB1),p(HGNC:RELA),a(GO:"NF-kappaB complex"), loc(GO:nucleus)) -> act(complex(GO:"NF-kappaB p50/p65 complex"))
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "In conditional neuronal NF-κB-deficient mice, loss of NF-κB signaling impaired synaptic
transmission, spatial memory formation, and plasticity"
SET Confidence = "Medium"
SET Species = "10090"
act(complex(GO:"NF-kappaB complex")) -> bp(MESH:"Synaptic Transmission")
act(complex(GO:"NF-kappaB complex")) -> path(MESH:"Spatial Memory")
act(complex(GO:"NF-kappaB complex")) -> bp(GO:"positive regulation of synaptic plasticity")
UNSET Species
UNSET Confidence

SET Evidence = "A number of physiological stimuli including membrane depolarization or glutamergic signal
transduction lead to rapid activation of the inducible NF-κB localized in the synapses,
cytoplasm and dendrites of the neurons"
SET Confidence = "High"
SET MeSHAnatomy = {"Synapses","Cytoplasm","Dendrites","Neurons"}
bp(GO:"membrane depolarization") -> act(complex(GO:"NF-kappaB complex"))
bp(GO:"synaptic transmission, glutamatergic") -> act(complex(GO:"NF-kappaB complex"))
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "Complete abrogation of the DNA binding ability of NFκB factors induces apoptosis of the neuronal cells"
SET Confidence = "Medium"
SET MeSHAnatomy = "Neurons"
act(complex(GO:"NF-kappaB complex"), ma(GO:"DNA binding")) -| bp(GO:"apoptotic process")
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = " Cell death is preceded by reduction in the
NF-κB regulated transcription of anti-apoptotic genes suggesting that a minimal threshold of
NF-κB activity is needed for neuronal survival"
SET Confidence = "High"
act(complex(GO:"NF-kappaB complex")) -| bp(GO:"neuron death")
UNSET Confidence

SET Evidence = "Several kinase pathways
including the calcium-calmodium dependent kinase-II (CaMK), the protein kinases-C (PKC)
and the ras/phosphatidylinositol 3-kinase (PI3K) pathways have been implicated in
activating neuronal NF-κB signaling"
SET Confidence = "High"
SET MeSHAnatomy = "Neurons"
bp(GO:"calmodulin dependent kinase signaling pathway") -> act(complex(GO:"NF-kappaB complex"))
act(p(FPLX:PKC)) -> act(complex(GO:"NF-kappaB complex"))
bp(GO:"phosphatidylinositol 3-kinase signaling") -> act(complex(GO:"NF-kappaB complex"))
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "Unlike neurons, NF-κB is present in the cytoplasm as an
inactive complex with the IκB proteins in glial cells under physiological conditions"
SET Confidence = "Low"
SET MeSHAnatomy = {"Cytoplasm","Neuroglia"}
a(MESH:"I-kappa B Proteins") -| act(complex(GO:"NF-kappaB complex"))
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "Various endogenous and exogenous stimuli
activate NF-κB enhancing transactivation of inflammatory molecules and production of free
radicals in glial cells"
SET Confidence = "High"
SET MeSHAnatomy = "Neuroglia"
act(complex(GO:"NF-kappaB complex")) -> bp(GO:"inflammatory response")
act(complex(GO:"NF-kappaB complex")) -> a(MESH:"Free Radicals")
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "Glutamate induced stimulation of cerebellar granule cells via the N-methyl-D-aspartate (NMDA)
receptor activate p65:p50 dimers and enhance transactivation of pro-apoptotic factors"
SET Confidence = "Medium"
SET Cell = "cerebellar granule cell"
a(CHEBI:"glutamate(1-)") -> act(a(MESH:"Receptors, N-Methyl-D-Aspartate"))
a(CHEBI:"glutamate(1-)") -> act(complex(p(HGNC:NFKB1),p(HGNC:RELA)))
a(MESH:"Receptors, N-Methyl-D-Aspartate") -> act(complex(p(HGNC:NFKB1),p(HGNC:RELA)))
act(complex(p(HGNC:NFKB1),p(HGNC:RELA))) -> bp(GO:"apoptotic process")
UNSET Cell
UNSET Confidence

SET MeSHDisease = "Alzheimer Disease"

SET Evidence = "A consequence of intracellular and parenchymal accumulation of NPs and NFTs is activation
of NF-κB in the neural and glial cells with subsequent protective or detrimental effects"
# NPs: neuritic plaques
# NFTS: neurofibrillary tangles
SET Confidence = "High"
SET MeSHAnatomy = {"Neurons","Neuroglia"}
a(GO:"neurofibrillary tangle") pos act(complex(GO:"NF-kappaB complex"))
path(MESH:"Plaque, Amyloid") pos act(complex(GO:"NF-kappaB complex"))
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = " In neuronal cells Aβ1-42
peptide has been shown to regulate APP and BACE1 proteins in NF-κB dependent manner"
SET Confidence = "High"
SET MeSHAnatomy = "Neurons"
a(CHEBI:"amyloid-beta polypeptide 42") reg p(HGNC:APP)
a(CHEBI:"amyloid-beta polypeptide 42") reg p(HGNC:BACE1)
a(CHEBI:"amyloid-beta polypeptide 42") -> act(complex(GO:"NF-kappaB complex"))
act(complex(GO:"NF-kappaB complex")) reg p(HGNC:APP)
act(complex(GO:"NF-kappaB complex")) reg p(HGNC:BACE1)
UNSET MeSHAnatomy
UNSET Confidence

UNSET MeSHDisease

SET Evidence = "Under physiological conditions activation of NF-κB by endogenous Aβ reduces βAPP,
BACE1 and the γ-secretase activity, thereby lowering Aβ processing and facilitating Aβ
homeostasis"
SET Confidence = "High"
a(CHEBI:"amyloid-beta") -> act(complex(GO:"NF-kappaB complex"))
act(complex(GO:"NF-kappaB complex")) -| p(HGNC:BACE1)
act(complex(GO:"NF-kappaB complex")) -| p(CONSO:"sAPP-beta")
act(complex(GO:"NF-kappaB complex")) -| bp(CONSO:"APP processing")
act(complex(GO:"NF-kappaB complex")) reg a(CHEBI:"amyloid-beta")
UNSET Confidence

SET MeSHDisease = "Alzheimer Disease"

SET Evidence = " However in AD, exposure to high Aβ concentrations upregulates NF-κB
activation increasing βAPP and Aβ processing, precipitating a feed-back loop that favor
exacerbated Aβ production "
SET Confidence = "Medium"
a(CHEBI:"amyloid-beta") pos act(complex(GO:"NF-kappaB complex"))
act(complex(GO:"NF-kappaB complex")) pos p(CONSO:"sAPP-beta")
act(complex(GO:"NF-kappaB complex")) pos bp(CONSO:"APP processing")
bp(CONSO:"APP processing") -> a(CHEBI:"amyloid-beta")
UNSET Confidence

SET Evidence = "Aβ has been shown to upregulate APOE in astroglial cells.
This upregulation was inhibited by decoy-κB nucleotides supporting a critical role for NFκB in APOE function"
SET Confidence = "High"
SET MeSHAnatomy = "Astrocytes"
a(CHEBI:"amyloid-beta") -> act(p(HGNC:APOE))
complex(GO:"NF-kappaB complex") -- act(p(HGNC:APOE))
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = " Stimulation with the Aβ25-35 fragments induces
secretion of cytokines such as TNF-α and of neurotrophic factors such as nerve growth
factor (NGF) and brain derived nerve factor (BDNF) in NF-κB-dependent manner"
SET Confidence = "High"
composite(complex(GO:"NF-kappaB complex"),a(MESH:"amyloid beta-protein (25-35)")) -> sec(p(HGNC:TNF))
composite(complex(GO:"NF-kappaB complex"),a(MESH:"amyloid beta-protein (25-35)")) -> sec(p(HGNC:NGF))
composite(complex(GO:"NF-kappaB complex"),a(MESH:"amyloid beta-protein (25-35)")) -> sec(p(HGNC:BDNF))
UNSET Confidence

SET Evidence = "Stimulation of neuronal cells by TNF-α has been shown to upregulate transactivation of
anti-apoptotic gene products and neurotrophins such as Bcl-2 and NGF respectively"
SET Confidence = "Medium"
SET MeSHAnatomy = "Neurons"
p(HGNC:TNF) -> p(HGNC:NGF)
p(HGNC:TNF) -> p(HGNC:BCL2)
p(HGNC:BCL2) -| bp(GO:"apoptotic process")
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "Decoy κB nucleotides mediate cell death by blocking neurotrophins and anti-apoptotic factors
supporting an essential role for NF-κB in the neuroprotective process "
SET Confidence = "High"
complex(GO:"NF-kappaB complex") -> bp(MESH:Neuroprotection)
UNSET Confidence

SET Evidence = " In primary neuronal cells, exposure to Aβ25-35 peptide increase NF-κB mediated transactivation of
manganese superoxide dismutase (Mn-SOD), suppress peroxinitrite production and inhibit
membrane depolarization, thereby preventing apoptosis induced by oxidative stress"
SET Confidence = "High"
SET MeSHAnatomy = "Neurons"
a(MESH:"amyloid beta-protein (25-35)") -> act(complex(GO:"NF-kappaB complex"))
act(complex(GO:"NF-kappaB complex")) -> act(p(HGNC:SOD2))
act(complex(GO:"NF-kappaB complex")) -| a(CHEBI:peroxynitrite)
act(complex(GO:"NF-kappaB complex")) -| bp(GO:"membrane depolarization")
act(complex(GO:"NF-kappaB complex")) -| bp(GO:"apoptotic process")
bp(MESH:"Oxidative Stress") -> bp(GO:"apoptotic process")
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = " In metabotrophic glutamate receptor-5 (mGlu5) agonist pretreated primary cortical neurons or
neuroblastoma cells, Aβ induced toxicity was suppressed by selective activation of c-rel
containing NF-κB dimers and transactivation of anti-apoptotic genes, Mn-SOD and Bcl-Xl [26] (Figs 1B, 2A)"
SET Confidence = "Medium"
SET MeSHAnatomy = "Neurons"
p(HGNC:GRM5) -> complex(p(HGNC:REL),a(GO:"NF-kappaB complex"),a(GO:"NF-kappaB complex"))
p(HGNC:GRM5) -> act(p(HGNC:SOD2))
p(HGNC:GRM5) -> act(p(HGNC:BCL2L1))
complex(p(HGNC:REL),a(GO:"NF-kappaB complex"),a(GO:"NF-kappaB complex")) -| act(a(CHEBI:"amyloid-beta"))
act(p(HGNC:SOD2)) -| act(a(CHEBI:"amyloid-beta"))
act(p(HGNC:BCL2L1)) -| act(a(CHEBI:"amyloid-beta"))
UNSET MeSHAnatomy
UNSET Confidence

