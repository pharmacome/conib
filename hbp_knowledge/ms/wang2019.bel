SET Citation = {"PubMed", "30967783"}

SET Evidence = "The CNS contains several resident macrophages, such as microglia and non-parenchymal macrophages, in the choroid plexus, perivascularspace,and meninges,which function to maintain CNS homeostasis(Prinzetal.,2011)."
SET Confidence = "Medium"
SET Anatomy = "central nervous system"
SET MeSHAnatomy = {"Choroid Plexus","Meninges"}
SET Anatomy = "perivascular space"
a(MESH:Microglia) reg bp(MESH:Homeostasis)
a(MESH:Macrophages) reg bp(MESH:Homeostasis)
UNSET Anatomy
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "Alterations in CNS homeostasis lead to the recruitment of peripheral blood-derived monocytes (known as monocyte-derived macrophages) into the CNS, the activation of microglia, and the presence of foamy macrophages (Bogie et al., 2014; Kuhlmann et al., 2017; Mrdjen et al., 2018; Zéphir, 2018)."
SET Confidence = "High"
SET Anatomy = "central nervous system"
bp(MESH:Homeostasis) -> a(MESH:Monocytes)
bp(MESH:Homeostasis) -> bp(GO:"microglial cell activation")
UNSET Anatomy
UNSET Confidence

SET Evidence = "Monocyte-derived macrophages and CNS-resident phagocytic cells can promote neuroinﬂammation, thereby inducing MS, but they can also play neuroprotective and anti-inﬂammatory roles, depending on many factors."
SET Confidence = "Medium"
SET Cell = "neuron"
a(MESH:Macrophages) -> bp(GO:"neuroinflammatory response")
bp(GO:"neuroinflammatory response") -> path(DOID:"multiple sclerosis")
a(MESH:Macrophages) -> path(DOID:"multiple sclerosis")
UNSET Cell
a(MESH:Macrophages) -> bp(MESH:Neuroprotection)
a(MESH:Macrophages) -| path(MESH:Inflammation)
SET Anatomy = "central nervous system"
SET Cell = "neuron"
a(MESH:Phagocytes) -> bp(GO:"neuroinflammatory response")
bp(GO:"neuroinflammatory response") -> path(DOID:"multiple sclerosis")
a(MESH:Phagocytes) -> path(DOID:"multiple sclerosis")
UNSET Cell
a(MESH:Phagocytes) -> bp(MESH:Neuroprotection)
a(MESH:Phagocytes) -| path(MESH:Inflammation)
UNSET Anatomy
UNSET Confidence

SET Evidence = "Studies have demonstrated that microglia mediate synaptic pruning and trophic-factor production through fractalkine-CX3CR1 signaling in neurodevelopment (Tremblay et al., 2010; Ransohoﬀ and El Khoury, 2014; Schafer and Stevens, 2015; Miyamoto et al., 2016)"
SET Confidence = "High"
a(MESH:Microglia) -> complex(p(HGNC:CX3CL1),p(HGNC:CX3CR1))
complex(p(HGNC:CX3CL1),p(HGNC:CX3CR1)) -> bp(MESH:"Neuronal Plasticity")
complex(p(HGNC:CX3CL1),p(HGNC:CX3CR1)) -> bp(GO:"generation of neurons")
a(MESH:Microglia) -> bp(MESH:"Neuronal Plasticity")
a(MESH:Microglia) -> bp(GO:"generation of neurons")
UNSET Confidence

SET Evidence = "Fractalkine receptor deﬁcient mice showed decreased neuron survival in layer V cortical neurons during the ﬁrst postnatal week because of the reduced secretion of insulin-like growth factor from microglia (Ueno et al., 2013)."
#first postnatal week
SET Confidence = "High"
SET Species = "10090"
SET Anatomy = "cerebral cortex"
SET Cell = "neuron"
p(HGNC:CX3CR1) -> bp(MESH:"Cell Survival")
a(MESH:Microglia) -> p(HGNC:IGF1)
p(HGNC:IGF1) -> bp(MESH:"Cell Survival")
UNSET Cell
UNSET Anatomy
UNSET Species
UNSET Confidence

SET Evidence = "Furthermore, microglia depletion by the Cre-Lox technique led to deﬁciencies in multiple learning tasks, as well as a signiﬁcant reduction in learning related synapse formation (Parkhurst et al., 2014)."
SET Confidence = "High"
a(MESH:Microglia) -> bp(GO:"learning or memory")
a(MESH:Microglia) -> a(GO:synapse)
a(GO:synapse) -> bp(GO:"learning or memory")
UNSET Confidence

SET Evidence = "In addition, microglial-secreted BDNF could increase the activity of neuronal tropomyosin-related kinase receptor B, an important modulator in synaptic plasticity,prompting a crucial role of BDNF signaling in learning and memory-related synapse formation (Parkhurst etal.,2014)."
SET Confidence = "High"
a(MESH:Microglia) -> p(HGNC:BDNF)
p(HGNC:BDNF) -> act(p(HGNC:NTRK2))
p(HGNC:NTRK2) reg bp(MESH:"Neuronal Plasticity")
a(MESH:Microglia) -> bp(MESH:"Neuronal Plasticity")
p(HGNC:BDNF) -> a(GO:synapse)
a(GO:synapse) -> bp(GO:"learning or memory")
p(HGNC:BDNF) -> bp(GO:"learning or memory")
UNSET Confidence

SET Evidence = "Microglia may communicate with neurons;changes in the physiological environment such as chronic stress and light deprivation can cause hyper-ramiﬁcation of microglia and more frequent neuron–microglia contacts (Torres-Platas et al., 2014; Welberg, 2014; Yirmiya et al., 2015)."
SET Confidence = "Medium"
bp(GO:"cellular response to stress") -> bp(GO:"microglial cell activation")
path(MESH:Light) -| bp(GO:"microglial cell activation")
bp(GO:"microglial cell activation") -> composite(a(MESH:Microglia),a(MESH:Neurons))
bp(GO:"cellular response to stress") -> composite(a(MESH:Microglia),a(MESH:Neurons))
path(MESH:Light) -| composite(a(MESH:Microglia),a(MESH:Neurons))
UNSET Confidence

SET Evidence = "Moreover, microglia act as scavengers in the adult CNS to remove damaged components, apoptotic cells, and misfolded proteins (Sierra et al., 2010)."
SET Confidence = "High"
SET Anatomy = "adult central nervous system"
a(MESH:Microglia) -> bp(GO:"apoptotic cell clearance")
a(MESH:Microglia) -> deg(p(MESH:Proteins,pmod(CONSO:misfolded)))
UNSET Anatomy
UNSET Confidence

SET Evidence = "Once “danger” signals are detected in the surrounding environment, microglia rapidly change into a reactive phenotype, characterized by larger soma, and release various complement factors to facilitate neuron and tissue repair (Hanisch and Kettenmann, 2007; Graeber and Streit, 2010; Kettenmannetal.,2011)."
#NCIT:C118474 = Damage-Associated Molecular Pattern Molecule
#NCIT:C53611 = repair
