m(MIRBASE:"hsa-mir-30e") => p(CONSO:"alpha-synuclein aggregates")
p(CONSO:"alpha-synuclein aggregates") -- path(DOID:"Parkinson's disease")
composite(p(CONSO:"alpha-synuclein aggregates"),m(MIRBASE:"hsa-mir-7-1")) -> path(DOID:"Parkinson's disease")
composite(p(CONSO:"alpha-synuclein aggregates"),m(MIRBASE:"hsa-mir-30e")) -> path(DOID:"Parkinson's disease")
UNSET Species
UNSET Confidence

