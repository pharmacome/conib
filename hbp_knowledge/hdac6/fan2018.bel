p(HGNC:GSK3B,pmod(Ph,Tyr,216)) -- act(p(HGNC:GSK3B))

SET Evidence = "As demonstrated, phosphorylation
of tau Ser396 and Ser404 significantly increased in the
cells transfected with the plasmids, but no increases or
mild increases in the phosphorylation of other sites
(Ser262 and Ser356) were observed (Fig. 5a)."
p(HGNC:MAPT,var("p.Pro301Leu")) -> p(HGNC:MAPT,pmod(Ph,Ser,396))
p(CONSO:"APP695",var("?")) -> p(HGNC:MAPT,pmod(Ph,Ser,396))
p(HGNC:MAPT,var("p.Pro301Leu")) -> p(HGNC:MAPT,pmod(Ph,Ser,404))
p(CONSO:"APP695",var("?")) -> p(HGNC:MAPT,pmod(Ph,Ser,404))
