# Statements #
##############
SET Citation = {"PubMed", "30008470"}

SET Species = "10090"

SET Evidence = "As shown in Fig. 1, we found that this sub-chronic alcohol treatment increased Adnp mRNA levels
in the dorsal hippocampus 24 h, but not 2 h after the last alcohol injection, in both male and female mice (one-way ANOVA; males:
F(2,10) = 5.94, p < 0.05; post-hoc, p < 0.02. Females: F(2,10) = 5.22, p < 0.05, post hoc, p < 0.05)"
# Short exposure to alcohol (7 days)
SET Confidence = "High"
SET MeSHAnatomy = "Hippocampus"
SET Gender = {"Female","Male"}
path(MESH:"Alcohol Drinking") -> r(HGNC:ADNP)
UNSET MeSHAnatomy
UNSET Gender
UNSET Confidence

SET Evidence = "In addition, Adnp expression was increased at the 24 h time point in the NAc of female, but not
male mouse (one-way ANOVA; females: F(2,9) = 10.87, p < 0.005; post hoc: p < 0.005; males: p > 0.05)."
# NAc: Nucleus Accumbens
# Short exposure to alcohol (7 days)
SET Confidence = "High"
SET MeSHAnatomy = "Nucleus Accumbens"
SET Gender = "Female"
path(MESH:"Alcohol Drinking") -> p(HGNC:ADNP)
SET Gender = "Male"
path(MESH:"Alcohol Drinking") cnc p(HGNC:ADNP)
UNSET MeSHAnatomy
UNSET Gender
UNSET Confidence

SET Evidence = "We found no changes in the expression of Adnp in the VTA (p’s > 0.05)."
# VTA: Ventral Tegmental Area
# Short exposure to alcohol (7 days)
SET Confidence = "High"
SET MeSHAnatomy = "Ventral Tegmental Area"
SET Gender = {"Female","Male"}
path(MESH:"Alcohol Drinking") cnc p(HGNC:ADNP)
UNSET MeSHAnatomy
UNSET Gender
UNSET Confidence

SET Evidence = "We found that among male mice, Adnp mRNA levels were increased in the dorsal hippocampus after a 24 h alcohol drinking
session, but the level returned to baseline (water-drinking controls) after 24 h of withdrawal (Fig. 2b; one-way ANOVA:
F(2,13) = 5.32, p < 0.05, post hoc comparisons, p’s < 0.05)"
# Long-term consumption of excessive alcohol (5 weeks)
SET Confidence = "High"
SET MeSHAnatomy = "Hippocampus"
SET Gender = "Male"
path(MESH:"Alcohol Drinking") -> r(HGNC:ADNP)
UNSET MeSHAnatomy
UNSET Gender
UNSET Confidence

SET Evidence = "However, in female mice, the expression of Adnp was reduced after a 24-h
alcohol-drinking session, and this reduction persisted after a 24-h
withdrawal (Fig. 2c; one-way ANOVA: F(2,13) = 7.53, p < 0.01, post hoc comparisons, p’s < 0.05)."
# Long-term consumption of excessive alcohol (5 weeks)
SET Confidence = "High"
SET MeSHAnatomy = "Hippocampus"
SET Gender = "Female"
path(MESH:"Alcohol Drinking") -| p(HGNC:ADNP)
UNSET MeSHAnatomy
UNSET Gender
UNSET Confidence

SET Evidence = "No changes in Adnp levels were detected in the VTA or NAc (p’s > 0.05)."
# Long-term consumption of excessive alcohol (5 weeks)
SET Confidence = "High"
SET MeSHAnatomy = {"Ventral Tegmental Area","Nucleus Accumbens"}
SET Gender = {"Female","Male"}
path(MESH:"Alcohol Drinking") cnc p(HGNC:ADNP)
UNSET MeSHAnatomy
UNSET Gender
UNSET Confidence

SET Evidence = "Originally, a reduction of 42% in the cortex, of 38% in cerebellum, and of
50% in hippocampus was observed in the levels of Adnp mRNA in Adnp+/− mice compared with Adnp intact mice"
# Adnp+/− refers to Haploinsufficiency
SET Confidence = "High"
SET MeSHAnatomy = {"Hippocampus","Cerebral Cortex","Cerebellum"}
p(HGNC:ADNP,pmod(MESH:Haploinsufficiency)) -| r(HGNC:ADNP)
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "Importantly, Adnp+/− females showed higher alcohol consumption and preference, compared to female
Adnp+/+ controls, whereas no difference was observed in males."
SET Confidence = "High"
SET Gender = "Female"
p(HGNC:ADNP,pmod(MESH:Haploinsufficiency)) -> path(MESH:"Alcohol Drinking")
SET Gender = "Male"
p(HGNC:ADNP,pmod(MESH:Haploinsufficiency)) cnc path(MESH:"Alcohol Drinking")
UNSET Gender
UNSET Confidence

SET Evidence = "Together, our results indicate that reduced Adnp gene dosage leads to increased
alcohol intake in female mice."
SET Confidence = "High"
SET Gender = "Female"
g(HGNC:ADNP) neg path(MESH:"Alcohol Drinking")
UNSET Gender
UNSET Confidence

SET Evidence = "As shown in Fig. 3e, f, we found no difference between genotypes
in saccharin or quinine intake, suggesting that the effect of Adnp deficiency is specific to alcohol, and does not apply to
sweet reinforcers or bitter-taste solutions"
SET Confidence = "High"
p(HGNC:ADNP,pmod(MESH:Haploinsufficiency)) -> path(MESH:"Alcohol Drinking")
p(HGNC:ADNP,pmod(MESH:Haploinsufficiency)) cnc tloc(a(CHEBI:saccharin),fromLoc(GO:"extracellular space"),toLoc(GO:intracellular))
p(HGNC:ADNP,pmod(MESH:Haploinsufficiency)) cnc tloc(a(CHEBI:quinine),fromLoc(GO:"extracellular space"),toLoc(GO:intracellular))
UNSET Confidence

SET Evidence = "NAP has been shown to ameliorate Adnp deficiencies in the haploinsufficient mouse model [25], mechanistically by enhancing ADNP association with its microtubule and autophagy targets"
#9832404 : NAP peptide
SET Confidence = "High"
a(PUBCHEM:9832404) -| p(HGNC:ADNP,pmod(MESH:"Haploinsufficiency"))
a(PUBCHEM:9832404) -> complex(p(HGNC:ADNP),a(GO:microtubule))
UNSET Confidence

SET Evidence = "As Fig. 4b depicts, over the first 2 weeks of drinking (no treatment or
intranasal vehicle treatment) Adnp+/− females showed higher
alcohol consumption as compared to their Adnp+/+ littermates, replicating our results above."
SET Confidence = "High"
SET Gender = "Female"
p(HGNC:ADNP,pmod(MESH:Haploinsufficiency)) -> path(MESH:"Alcohol Drinking")
UNSET Gender
UNSET Confidence

SET Evidence = "Strikingly, NAP treatment reduced alcohol consumption in Adnp+/− mice, which did not differ from Adnp+/+ controls."
SET Confidence = "High"
composite(p(HGNC:ADNP,pmod(MESH:Haploinsufficiency)),a(PUBCHEM:9832404)) -| path(MESH:"Alcohol Drinking")
UNSET Confidence

SET Evidence = "These results indicate that restoration of ADNP function by NAP administration rescues the wild type phenotype of the Adnp+/−
mice, by reversing the abnormal increase in alcohol intake seen in
Adnp haploinsufficient female mice"
SET Confidence = "High"
SET Gender = "Female"
a(PUBCHEM:9832404) -> act(p(HGNC:ADNP))
p(HGNC:ADNP,pmod(MESH:Haploinsufficiency)) -| path(MESH:"Alcohol Drinking")
UNSET Gender
UNSET Confidence

SET Evidence = "For example, one of these common genes was transcription factor AP2 beta (TFAP2b), which is downregulated by chronic alcohol
exposure [41]. This gene had a 3.8-fold increased expression in female Adnp+/− mice, and in contrast, a five-fold decreased
expression in male Adnp+/− mice, as compared to sex-matched Adnp+/+controls."
SET Confidence = "High"
path(MESH:"Alcohol Drinking") -| p(HGNC:TFAP2B)
SET Gender = "Female"
p(HGNC:ADNP,pmod(MESH:"Haploinsufficiency")) -> p(HGNC:TFAP2B)
SET Gender = "Male"
p(HGNC:ADNP,pmod(MESH:"Haploinsufficiency")) -| p(HGNC:TFAP2B)
UNSET Gender
UNSET Confidence

UNSET Species
