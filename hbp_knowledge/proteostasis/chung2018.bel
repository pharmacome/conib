p(HGNC:TARDBP,pmod(HBP:misfolding)) pos path(MESH:"Amyotrophic Lateral Sclerosis")
p(HGNC:TARDBP,pmod(HBP:misfolding)) pos path(MESH:"Frontotemporal Dementia")

