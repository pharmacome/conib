SET Confidence = "High"
bp(GO:macroautophagy) -> sec(a(CHEBI:"amyloid-beta"))
sec(a(CHEBI:"amyloid-beta")) -| path(HBP:Neurodegeneration)
sec(a(CHEBI:"amyloid-beta")) -> bp(GO:memory)
UNSET Confidence

