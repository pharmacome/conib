SET Confidence = "High"
p(HGNC:ATG7) -> bp(GO:macroautophagy)
bp(GO:macroautophagy) -> sec(a(CHEBI:"amyloid-beta"))
bp(GO:macroautophagy) -> path(MESH:"Plaque, Amyloid")
UNSET Confidence

SET Evidence = "This inhibition of Aβ secretion during macroautophagy deficiency results in
