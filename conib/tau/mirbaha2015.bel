# Statements #
##############

SET Citation = {"PubMed","25887395"}

#TauRD: The Tau repeat domain (RD) (13), comprising amino acids 243–375
SET Evidence = "Tau RD fibrillizes very efficiently in vitro, making it ideal for
fundamental studies (13)."
SET Confidence = "High"
p(HGNC:MAPT, frag("243_375")) -> a(CONSO:"Tau fibrils")
UNSET Confidence

SET Evidence = "Heparan sulfate proteoglycans (HSPGs) are cell surface pro-
teins that bind Tau fibrils and are required for their uptake and
seeding (10). "
#Syndecans: Syndecan heparan sulfate proteoglycans
SET Confidence = "High"
complex(p(HGNCGENEFAMILY:"Syndecans"),a(CONSO:"Tau fibrils")) -> tloc(a(CONSO:"Tau fibrils"), MESH:"Extracellular Space", MESH:"Intracellular Space")
complex(p(HGNCGENEFAMILY:"Syndecans"),a(CONSO:"Tau fibrils")) -> a(CONSO:"Tau aggregates")
UNSET Confidence

SET Evidence = "Heparin blocks Tau binding by masking HSPG binding
sites on Tau, and sodium chlorate inhibits HSPG sulfation."
SET Confidence = "Medium"
a(CHEBI:heparin) -| complex(p(HGNCGENEFAMILY:"Syndecans"),a(CONSO:"Tau fibrils"))
a(CHEBI:"sodium chlorate") -| complex(p(HGNCGENEFAMILY:"Syndecans"),a(CONSO:"Tau fibrils"))
a(CHEBI:"sodium chlorate") -| p(HGNCGENEFAMILY:"Syndecans",pmod(Sulf))
UNSET Confidence

SET Evidence = " Within
the limits of our detection, neither monomer nor dimer was
internalized (Fig. 5, A and B). By contrast, trimers and larger
species were readily internalized, with trimer uptake being
slightly less efficient (Fig. 5, A and B). We did not observe an
upper limit of size in terms of uptake."
SET Confidence = "Medium"
p(HGNC:MAPT) cnc tloc(a(CONSO:"Tau fibrils"), MESH:"Extracellular Space", MESH:"Intracellular Space")
complex(p(HGNC:MAPT),p(HGNC:MAPT)) cnc tloc(a(CONSO:"Tau fibrils"), MESH:"Extracellular Space", MESH:"Intracellular Space")
complex(p(HGNC:MAPT),p(HGNC:MAPT),p(HGNC:MAPT)) -> tloc(a(CONSO:"Tau fibrils"), MESH:"Extracellular Space", MESH:"Intracellular Space")
UNSET Confidence

SET Evidence = "Only Tau RD oligomers of
n  3 increased luciferase signal, defining these assemblies as
the minimal size sufficient for spontaneous cell uptake and
seeding (Fig. 6C)."
SET Confidence = "Medium"
complex(p(HGNC:MAPT, frag("243_375")),p(HGNC:MAPT, frag("243_375")),p(HGNC:MAPT, frag("243_375"))) -> a(CONSO:"Tau aggregates")
complex(p(HGNC:MAPT, frag("243_375")),p(HGNC:MAPT, frag("243_375")),p(HGNC:MAPT, frag("243_375"))) -> tloc(a(CONSO:"Tau fibrils"), MESH:"Extracellular Space", MESH:"Intracellular Space")
UNSET Confidence

SET Evidence = "Whereas control brain homogenate exhibited mainly Tau
monomer, the AD brain contained a range of Tau assemblies
ranging from n = 1 to n > 20 (Fig. 7, A and B). "
SET Confidence = "High"
SET MeSHAnatomy = "Brain"
path(MESH:"Alzheimer Disease") pos a(CONSO:"Tau oligomers")
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "Next we analyzed fractions for seeding activity in the P301S-
Nluc/Cluc HEK293 cell lines by a split-luciferase complemen-
tation assay, comparing AD and control brains. Trimers and
larger BD Tau assemblies induced intracellular aggregation, but
not monomer or dimer. "
# BD: Brain-derived
SET Confidence = "Medium"
SET MeSHDisease = "Alzheimer Disease"
SET MeSHAnatomy = "Brain"
p(HGNC:MAPT) cnc a(CONSO:"Tau aggregates")
complex(p(HGNC:MAPT),p(HGNC:MAPT)) cnc a(CONSO:"Tau aggregates")
complex(p(HGNC:MAPT),p(HGNC:MAPT),p(HGNC:MAPT)) -> a(CONSO:"Tau aggregates")
UNSET MeSHDisease
UNSET MeSHAnatomy
UNSET Confidence

SET Evidence = "To test whether HSPGs also mediate
BD Tau seeding, we titrated heparin and sodium chlorate,
which inhibited seeding of all species (Fig. 8D)."
# BD: Brain-derived
SET Confidence = "High"
a(CHEBI:heparin) -| a(CONSO:"Tau aggregates")
a(CHEBI:"sodium chlorate") -| a(CONSO:"Tau aggregates")
UNSET Confidence
